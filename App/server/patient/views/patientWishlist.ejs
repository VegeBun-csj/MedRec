                        <span>钱包</span>
        
                    </a>
                </li>
        
        
        
            </ul><!--/.nav-list-->
    
        
            <div id="sidebar-collapse"><i class="icon-double-angle-left"></i></div>
        </div>
        <div id="main-content" class="clearfix">
            <div id="page-content" class="clearfix" style="margin-top: 45px;">
             <div class="page-header position-relative">
              <h1 style="color: #2679b5;">病人端<small><i class="icon-double-angle-right"></i> 收藏</small></h1>
            </div>
            <input type="hidden" name="pid" value="<%=pid%>">
			<input type="hidden" name="hid" value="<%=hid%>">
            <button class="btn btn-success" onclick="deleteWish()"><i class="icon-trash icon-4x icon-only">delete</i></button>
            <ul>
                
                  <%wish_medicine.forEach(function(item){%>

                    <li class="imglist">

                        <a href="/patient/medicineDetail?mid=<%=item.mid%>&pid=<%=pid%>&hid=<%=hid%>">
                            <label><input type="checkbox" class="checkb" value="<%=item.wish_id%>"><span class="lbl"></span></label>
                        <img src="/medicinePIc/<%=item.m_picture%>"  alt="" />
                        <div class="right">
                            <p><%=item.m_name%></p>
                            <span class="red"><%=item.m_price%></span>
                        
                            <!-- <input type="checkbox" /> -->
                        </div>
                        </a>
                    </li>
                    <%})%>
            
            </ul>
        </div>
        </div>
        <script type="text/javascript">


            function deleteWish() {
                

               

                var pid =parseInt( $('input[name = "pid"]').val());
				var hid = parseInt($('input[name = "hid"]').val())
                var result="";
                var count=0;
                $(".checkb").each(function(){
                    if($(this).is(':checked')){
                        result+=$(this).val()+",";
                        count++;
                    }else{}
                });
                if(!confirm("确定删除这"+count+"件想要的吗?")){
                    return;
                }
                window.location.href= "/patient/delWish?tag="+result+"&pid="+pid+"&hid="+hid;
                
            
            }
           
         </script>
    </body>
</html>
